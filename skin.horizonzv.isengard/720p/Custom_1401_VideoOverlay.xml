<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1401">
	<defaultcontrol always="true">2</defaultcontrol>
	<!-- <zorder>1</zorder> -->
	<visible>Player.HasVideo</visible>
	<controls>
		<control type="group">
			<left>0</left>
			<top>-5</top>
			<include>openclose</include>
			<include>opencloseslidedown</include>
			<visible>!Window.IsActive(visualisation)+!Window.IsActive(Home)+!Window.IsVisible(playercontrols)+!VideoPlayer.IsFullscreen+!Window.IsVisible(screencalibration)+![Window.IsVisible(videos)+Skin.HasSetting(CoverWallFull)]+!Control.HasFocus(666)</visible>
			<control type="group" id="501">
				<left>0</left>
				<top>-6</top>
				<control type="image">
					<left>-10</left>
					<top>3</top>
					<width>1300</width>
					<height>40</height>
					<texture background="true">BlackBack.png</texture>
				</control>
				<control type="image">
					<left>-10</left>
					<top>43</top>
					<width>1300</width>
					<height>10</height>
					<colordiffuse>menudiffuse</colordiffuse>
					<texture background="true" flipy="true">Shadow.png</texture>
				</control>
				<control type="image">
					<left>0</left>
					<top>43</top>
					<width>1280</width>
					<height>1</height>
					<texture background="true">Separator2.png</texture>
				</control>
				<control type="grouplist">
					<left>10</left>
					<top>11</top>
					<width>1000</width>
					<height>30</height>
					<itemgap>2</itemgap>
					<orientation>horizontal</orientation>
					<include>RecordInfo</include>
					<control type="image"> <!-- fake -->
						<description>Separator</description>
						<width>3</width>
						<height>30</height>
						<texture>-</texture>
					</control>
					<include>NavInfoPvrContent</include>
					<control type="label">
						<description>Navigation label</description>
						<width>auto</width>
						<label>$INFO[System.CurrentWindow]$INFO[Container.FolderName, - ,]</label>
						<font>impacted24</font>
						<textcolor>white</textcolor>
						<visible>Skin.HasSetting(NavInfo)</visible>
						<visible>!Window.IsActive(pvr)</visible>
						<visible>!Window.IsActive(SkinSettings)</visible>
					</control>
					<control type="label">
						<description>Navigation label</description>
						<width>auto</width>
						<label>20077</label>
						<font>impacted24</font>
						<textcolor>white</textcolor>
						<visible>Window.IsActive(SkinSettings)</visible>
					</control>
					<control type="image"> <!-- fake -->
						<description>Separator</description>
						<width>5</width>
						<height>30</height>
						<texture>-</texture>
					</control>
					<control type="image">
						<description>Separator</description>
						<width>1</width>
						<height>30</height>
						<texture>Separator3.png</texture>
					</control>
					<control type="image"> <!-- fake -->
						<description>Separator</description>
						<width>0</width>
						<height>30</height>
						<texture>-</texture>
					</control>
					<control type="image">
						<description>Overlay Watching</description>
						<width>30</width>
						<height>32</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$VAR[OSDTexture]</texture>
					</control>
					<control type="label">
						<description>Now Playing Info</description>
						<width>auto</width>
						<height>34</height>
						<textcolor>white</textcolor>
						<shadowcolor>-</shadowcolor>
						<label>$VAR[HomeOSDTitle]</label>
						<scroll>true</scroll>
					</control>
					<control type="label">
						<description>Now Playing Info</description>
						<width>auto</width>
						<height>34</height>
						<textcolor>white</textcolor>
						<shadowcolor>-</shadowcolor>
						<label>$INFO[VideoPlayer.Time, [COLOR=description]â€¢[/COLOR]  , ]$INFO[VideoPlayer.Duration,[COLOR=grey]/ [/COLOR], ]</label>
						<visible>!IsEmpty(Player.TimeRemaining)</visible>
					</control>
				</control>
			</control>
			<control type="label">
				<animation effect="slide" end="-20" condition="Skin.HasSetting(BatteryLevel)">Conditional</animation>
				<left>319r</left>
				<top>4</top>
				<width>310</width>
				<align>right</align>
				<aligny>center</aligny>
				<font>impacted24</font>
				<label>$INFO[System.Time]</label>
			</control>
			<control type="group">
				<left>1248</left>
				<top>2</top>
				<width>24</width>
				<height>14</height>
				<include>BatteryLevel</include>
			</control>
		</control>
	</controls>
</window>