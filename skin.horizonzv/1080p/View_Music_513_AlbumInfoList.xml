<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="AlbumInfoListView">
        <control type="group">
            <include>openclose</include>
            <visible>Control.IsVisible(513)+!Control.HasFocus(666)</visible>
            <control type="button" id="888">
                <onup>Control.SetFocus(9000)</onup>
                <ondown>Control.SetFocus(513)</ondown>
                <include>HiddenButton</include>
            </control>
            <control type="group">
                <include>BlackGrunge</include>
            </control>
            <control type="group">
                <left>15</left>
                <include>openclosebox</include>
                <visible>Control.IsVisible(513)+!Control.HasFocus(666)</visible>
                <include content="DialogInfoBackgroundParam">
                    <param name="DialogBackgroundLeft" value="120" />
                    <param name="DialogBackgroundTop" value="105" />
                    <param name="DialogBackgroundWidth" value="1650" />
                    <param name="DialogBackgroundHeight" value="893" />
                    <param name="DialogSeparatorTop" value="105" />
                    <param name="DialogSeparatorBottom" value="998" />
                    <param name="DialogShadowTop" value="1000" />
                    <param name="DialogHeaderLabel" value="-" />
                    <param name="DialogHeaderTop" value="-" />
                    <param name="DialogHeaderId" value="-" />
                </include>
                <control type="list" id="513">
                    <left>135</left>
                    <top>135</top>
                    <width>1110</width>
                    <height>842</height>
                    <onleft>9000</onleft>
                    <onright condition="Skin.HasSetting(HideJumpSMSBar)+Control.IsVisible(60)">60</onright>
                    <onright condition="Skin.HasSetting(HideJumpSMSBar)+!Control.IsVisible(60)">SetFocus(732)</onright>
                    <onright condition="!Skin.HasSetting(HideJumpSMSBar)">15000</onright>
                    <onup>513</onup>
                    <ondown>513</ondown>
                    <viewtype label="$LOCALIZE[10523]">list</viewtype>
                    <pagecontrol>60</pagecontrol>
                    <scrolltime>200</scrolltime>
                    <visible>Container.Content(Albums)</visible>
                    <itemlayout height="60" width="1110">
                        <control type="label">
                            <left>15</left>
                            <top>0</top>
                            <width>1050</width>
                            <height>60</height>
                            <textcolor>grey</textcolor>
                            <align>left</align>
                            <aligny>center</aligny>
                            <label>$INFO[ListItem.Label]</label>
                        </control>
                        <control type="label">
                            <left>45</left>
                            <top>0</top>
                            <width>1050</width>
                            <height>60</height>
                            <textcolor>grey</textcolor>
                            <align>right</align>
                            <aligny>center</aligny>
                            <label>$INFO[ListItem.Label2]</label>
                        </control>
                    </itemlayout>
                    <focusedlayout height="60" width="1110">
                        <control type="group">
                            <include>focuseffect</include>
                            <control type="image">
                                <left>0</left>
                                <top>0</top>
                                <width>1110</width>
                                <height>62</height>
                                <colordiffuse>grey1</colordiffuse>
                                <texture border="5">buttons/ButtonFOlightGlow.png</texture>
                                <visible>!Control.HasFocus(513)</visible>
                            </control>
                            <control type="image">
                                <left>0</left>
                                <top>0</top>
                                <width>1110</width>
                                <height>62</height>
                                <colordiffuse>grey1</colordiffuse>
                                <texture border="5">buttons/ButtonFOlightGlow.png</texture>
                                <visible>Control.HasFocus(513)</visible>
                            </control>
                            <control type="label">
                                <left>15</left>
                                <top>0</top>
                                <width>1095</width>
                                <height>60</height>
                                <textcolor>white</textcolor>
                                <shadowcolor>black</shadowcolor>
                                <align>left</align>
                                <aligny>center</aligny>
                                <label>$INFO[ListItem.Label]</label>
                            </control>
                            <control type="label">
                                <left>45</left>
                                <top>0</top>
                                <width>1050</width>
                                <height>60</height>
                                <textcolor>white</textcolor>
                                <shadowcolor>black</shadowcolor>
                                <align>right</align>
                                <aligny>center</aligny>
                                <label>$INFO[ListItem.Label2]</label>
                            </control>
                        </control>
                    </focusedlayout>
                </control>
                <control type="scrollbar" id="60">
                    <left>1245</left>
                    <top>138</top>
                    <height>840</height>
                    <onleft>513</onleft>
                    <onright>732</onright>
                    <showonepage>false</showonepage>
                    <orientation>vertical</orientation>
                </control>
                <control type="group">
                    <left>1290</left>
                    <top>150</top>
                    <control type="image">
                        <left>15</left>
                        <top>0</top>
                        <width>435</width>
                        <height>360</height>
                        <aspectratio aligny="bottom">keep</aspectratio>
                        <fadetime>FadeTime</fadetime>
                        <texture background="true">$INFO[ListItem.Icon]</texture>
                        <bordertexture border="8">ThumbShadow.png</bordertexture>
                        <bordersize>8</bordersize>
                    </control>
                    <control type="label">
                        <description>Description Header txt</description>
                        <left>15</left>
                        <top>360</top>
                        <width>435</width>
                        <height>38</height>
                        <label>21821</label>
                        <align>center</align>
                        <aligny>center</aligny>
                        <textcolor>selected</textcolor>
                    </control>
                    <control type="textbox">
                        <description>Description Value for Album</description>
                        <left>0</left>
                        <top>405</top>
                        <width>458</width>
                        <height>420</height>
                        <font>font16</font>
                        <pagecontrol>-</pagecontrol>
                        <textcolor>white</textcolor>
                        <label>$INFO[ListItem.Property(Album_Description)]</label>
                    </control>
                </control>
                <control type="group">
                    <left>0</left>
                    <top>22</top>
                    <include>CommonPageCount</include>
                </control>
                <control type="group">
                    <!-- Music Flagging -->
                    <left>202</left>
                    <top>1008</top>
                    <include>openclose</include>
                    <include>opencloseslideup</include>
                    <control type="grouplist">
                        <left>0</left>
                        <top>0</top>
                        <width>870</width>
                        <height>120</height>
                        <itemgap>4</itemgap>
                        <align>center</align>
                        <orientation>horizontal</orientation>
                        <include>FlagMusic</include>
                    </control>
                </control>
            </control>
            <include condition="!Skin.HasSetting(HideJumpSMSBar)">AlphaBarV</include>
        </control>
    </include>
</includes>
