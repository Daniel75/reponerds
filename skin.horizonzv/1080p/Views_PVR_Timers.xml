<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="LiveTVTimersView">
        <control type="group">
            <description>Timers group</description>
            <visible>Control.IsVisible(50)</visible>
            <include>openclose</include>
            <include>opencloseslideup</include>
            <control type="group">
                <left>120</left>
                <top>300</top>
                <control type="image">
                    <description>Separator image</description>
                    <left>-135</left>
                    <top>12</top>
                    <width>1950</width>
                    <height>975</height>
                    <colordiffuse>menudiffuse</colordiffuse>
                    <texture background="true">BlackBack.png</texture>
                </control>
                <control type="label">
                    <description>Channel header label</description>
                    <left>0</left>
                    <top>33</top>
                    <width>330</width>
                    <height>30</height>
                    <textcolor>white</textcolor>
                    <shadowcolor>-</shadowcolor>
                    <align>center</align>
                    <aligny>center</aligny>
                    <label>19029</label>
                </control>
                <control type="label">
                    <description>Title header label</description>
                    <left>330</left>
                    <top>33</top>
                    <width>450</width>
                    <height>30</height>
                    <textcolor>white</textcolor>
                    <shadowcolor>-</shadowcolor>
                    <align>center</align>
                    <aligny>center</aligny>
                    <label>369</label>
                </control>
                <control type="label">
                    <description>Schedule Time header label</description>
                    <left>870</left>
                    <top>33</top>
                    <width>450</width>
                    <height>30</height>
                    <textcolor>white</textcolor>
                    <shadowcolor>-</shadowcolor>
                    <align>center</align>
                    <aligny>center</aligny>
                    <label>31501</label>
                </control>
                <control type="label">
                    <description>Status header label</description>
                    <left>1410</left>
                    <top>33</top>
                    <width>225</width>
                    <height>30</height>
                    <textcolor>white</textcolor>
                    <shadowcolor>-</shadowcolor>
                    <align>center</align>
                    <aligny>center</aligny>
                    <label>126</label>
                </control>
                <control type="image">
                    <description>Separator image</description>
                    <left>-135</left>
                    <top>12</top>
                    <width>1950</width>
                    <height>1</height>
                    <texture background="true">Separator2.png</texture>
                </control>
                <control type="list" id="50">
                    <viewtype label="535">list</viewtype>
                    <left>0</left>
                    <top>82</top>
                    <width>1650</width>
                    <height>594</height>
                    <onup>50</onup>
                    <ondown>50</ondown>
                    <onleft>9000</onleft>
                    <onright>73</onright>
                    <pagecontrol>73</pagecontrol>
                    <scrolltime>200</scrolltime>
                    <itemlayout height="66">
                        <control type="image">
                            <left>330</left>
                            <top>0</top>
                            <width>450</width>
                            <height>57</height>
                            <colordiffuse>menudiffuse</colordiffuse>
                            <texture background="true" border="5">BlackBack.png</texture>
                        </control>
                        <control type="image">
                            <left>0</left>
                            <top>12</top>
                            <width>75</width>
                            <height>39</height>
                            <visible>!String.IsEmpty(ListItem.Date)</visible>
                            <texture background="true" border="1">$INFO[ListItem.Icon]</texture>
                            <aspectratio>keep</aspectratio>
                        </control>
                        <control type="label">
                            <left>75</left>
                            <top>0</top>
                            <width>225</width>
                            <height>60</height>
                            <align>left</align>
                            <aligny>center</aligny>
                            <selectedcolor>white</selectedcolor>
                            <info>ListItem.ChannelName</info>
                        </control>
                        <control type="label">
                            <left>330</left>
                            <top>0</top>
                            <width>450</width>
                            <height>60</height>
                            <align>center</align>
                            <aligny>center</aligny>
                            <selectedcolor>white</selectedcolor>
                            <info>ListItem.Label</info>
                        </control>
                        <control type="label">
                            <left>795</left>
                            <top>0</top>
                            <width>600</width>
                            <height>60</height>
                            <align>center</align>
                            <aligny>center</aligny>
                            <selectedcolor>white</selectedcolor>
                            <info>ListItem.Date</info>
                        </control>
                        <control type="label">
                            <left>1410</left>
                            <top>0</top>
                            <width>225</width>
                            <height>60</height>
                            <align>center</align>
                            <aligny>center</aligny>
                            <selectedcolor>white</selectedcolor>
                            <info>ListItem.Comment</info>
                        </control>
                    </itemlayout>
                    <focusedlayout height="66">
                        <control type="group">
                            <include>focuseffect</include>
                            <control type="image">
                                <left>0</left>
                                <top>0</top>
                                <width>1650</width>
                                <height>57</height>
                                <colordiffuse>reddiffuse</colordiffuse>
                                <texture>WhiteBack.png</texture>
                                <visible>Control.HasFocus(50)</visible>
                            </control>
                            <control type="image">
                                <left>330</left>
                                <top>0</top>
                                <width>450</width>
                                <height>57</height>
                                <colordiffuse>red</colordiffuse>
                                <texture diffuse="masks/banner_mask.png">WhiteBack.png</texture>
                                <visible>Control.HasFocus(50)</visible>
                            </control>
                            <control type="image">
                                <left>330</left>
                                <top>0</top>
                                <width>450</width>
                                <height>57</height>
                                <texture border="4">masks/Default_sharp.png</texture>
                            </control>
                            <control type="image">
                                <left>0</left>
                                <top>12</top>
                                <width>75</width>
                                <height>39</height>
                                <visible>!String.IsEmpty(ListItem.Date)</visible>
                                <texture background="true" border="1">$INFO[ListItem.Icon]</texture>
                                <aspectratio>keep</aspectratio>
                            </control>
                            <control type="label">
                                <left>75</left>
                                <top>0</top>
                                <width>225</width>
                                <height>60</height>
                                <align>left</align>
                                <aligny>center</aligny>
                                <selectedcolor>white</selectedcolor>
                                <info>ListItem.ChannelName</info>
                            </control>
                            <control type="label">
                                <left>330</left>
                                <top>0</top>
                                <width>450</width>
                                <height>60</height>
                                <align>center</align>
                                <aligny>center</aligny>
                                <selectedcolor>white</selectedcolor>
                                <info>ListItem.Label</info>
                            </control>
                            <control type="label">
                                <left>795</left>
                                <top>0</top>
                                <width>600</width>
                                <height>60</height>
                                <align>center</align>
                                <aligny>center</aligny>
                                <selectedcolor>white</selectedcolor>
                                <info>ListItem.Date</info>
                            </control>
                            <control type="label">
                                <left>1410</left>
                                <top>0</top>
                                <width>225</width>
                                <height>60</height>
                                <align>center</align>
                                <aligny>center</aligny>
                                <selectedcolor>white</selectedcolor>
                                <info>ListItem.Comment</info>
                            </control>
                        </control>
                    </focusedlayout>
                </control>
                <control type="scrollbar" id="73">
                    <left>1658</left>
                    <top>82</top>
                    <height>594</height>
                    <onleft>50</onleft>
                    <onright>noop</onright>
                    <showonepage>false</showonepage>
                    <orientation>vertical</orientation>
                </control>
                <control type="image">
                    <description>Separator image</description>
                    <left>82</left>
                    <top>810</top>
                    <width>1515</width>
                    <height>1</height>
                    <colordiffuse>88FFFFFF</colordiffuse>
                    <texture background="true">Separator2.png</texture>
                </control>
                <control type="label">
                    <description>Next timer date</description>
                    <left>82</left>
                    <top>818</top>
                    <width>1515</width>
                    <height>45</height>
                    <align>center</align>
                    <aligny>center</aligny>
                    <scroll>true</scroll>
                    <textcolor>white</textcolor>
                    <label>$INFO[PVR.NextTimer]</label>
                    <visible>PVR.HasTimer</visible>
                </control>
            </control>
        </control>
    </include>
</includes>
