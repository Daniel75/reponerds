<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="ArtistMediaListView">
        <control type="group">
            <include>openclose</include>
            <include>openclosevisible</include>
            <visible>Control.IsVisible(512)</visible>
            <control type="group">
                <animation effect="fade" start="100" end="0" time="200" condition="Control.HasFocus(666)">Conditional</animation>
                <include>WhiteGrunge</include>
            </control>
        </control>
        <control type="group">
            <include>openclose</include>
            <include>opencloseslideup</include>
            <visible>Control.IsVisible(512)</visible>
            <control type="group">
                <animation effect="fade" start="100" end="0" time="200" condition="Control.HasFocus(666)">Conditional</animation>
                <include>MirrorPanel</include>
            </control>
        </control>
        <control type="group">
            <include>openclose</include>
            <include>opencloseslideright</include>
            <visible>Control.IsVisible(512)</visible>
            <animation effect="fade" start="100" end="0" time="200" condition="Control.HasFocus(666)">Conditional</animation>
            <animation type="Conditional" condition="[ControlGroup(9000).HasFocus()|ControlGroup(90000).HasFocus()|ControlGroup(9001).HasFocus()| ControlGroup(9002).HasFocus()] + Skin.HasSetting(animation)">
                <effect type="zoom" center="750,750" start="100" end="105" time="250" />
                <effect type="slide" start="0" end="525" time="250" />
            </animation>
            <include>InfoListLeft</include>
            <control type="list" id="512">
                <left>105</left>
                <top>195</top>
                <width>870</width>
                <height>735</height>
                <onleft>9000</onleft>
                <onright condition="Skin.HasSetting(HideJumpSMSBar)+Control.IsVisible(60)">60</onright>
                <onright condition="Skin.HasSetting(HideJumpSMSBar)+!Control.IsVisible(60)">SetFocus(732)</onright>
                <onright condition="!Skin.HasSetting(HideJumpSMSBar)">15000</onright>
                <onup>512</onup>
                <ondown>512</ondown>
                <viewtype label="$LOCALIZE[544]">list</viewtype>
                <pagecontrol>60</pagecontrol>
                <scrolltime>200</scrolltime>
                <visible>Container.Content(artists)</visible>
                <itemlayout height="60" width="870">
                    <control type="label">
                        <left>15</left>
                        <top>3</top>
                        <width>780</width>
                        <height>60</height>
                        <textcolor>grey</textcolor>
                        <align>left</align>
                        <aligny>center</aligny>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <control type="label">
                        <left>82</left>
                        <top>3</top>
                        <width>750</width>
                        <height>60</height>
                        <align>right</align>
                        <aligny>center</aligny>
                        <shadowcolor>black</shadowcolor>
                        <label>$VAR[InfoWallBottomLabel1Var]</label>
                    </control>
                </itemlayout>
                <focusedlayout height="60" width="870">
                    <control type="group">
                        <include>focuseffect</include>
                        <control type="image">
                            <left>0</left>
                            <top>3</top>
                            <width>870</width>
                            <height>62</height>
                            <colordiffuse>grey2</colordiffuse>
                            <texture border="5">buttons/ButtonFOlightGlow.png</texture>
                            <visible>!Control.HasFocus(512)</visible>
                        </control>
                        <control type="image">
                            <left>0</left>
                            <top>3</top>
                            <width>870</width>
                            <height>62</height>
                            <colordiffuse>grey1</colordiffuse>
                            <texture border="5">buttons/ButtonFOlightGlow.png</texture>
                            <visible>Control.HasFocus(512)</visible>
                        </control>
                        <control type="label">
                            <left>15</left>
                            <top>3</top>
                            <width>780</width>
                            <height>60</height>
                            <align>left</align>
                            <aligny>center</aligny>
                            <shadowcolor>black</shadowcolor>
                            <label>$INFO[ListItem.Label]</label>
                        </control>
                        <control type="label">
                            <left>82</left>
                            <top>3</top>
                            <width>750</width>
                            <height>60</height>
                            <align>right</align>
                            <aligny>center</aligny>
                            <shadowcolor>black</shadowcolor>
                            <label>$VAR[InfoWallBottomLabel1Var]</label>
                        </control>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <left>964</left>
                <top>188</top>
                <height>742</height>
                <onleft>512</onleft>
                <onright>732</onright>
                <showonepage>false</showonepage>
                <orientation>vertical</orientation>
            </control>
        </control>
        <control type="group">
            <include>openclose</include>
            <include>opencloseslideleft</include>
            <visible>Control.IsVisible(512)</visible>
            <control type="group">
                <animation effect="fade" start="100" end="0" time="200" condition="Control.HasFocus(666)">Conditional</animation>
                <animation type="Conditional" condition="[ControlGroup(9000).HasFocus()|ControlGroup(90000).HasFocus()|ControlGroup(9001).HasFocus()| ControlGroup(9002).HasFocus()] + Skin.HasSetting(animation)">
                    <effect type="zoom" center="750,750" start="100" end="105" time="250" />
                    <effect type="slide" start="0" end="630" time="250" />
                    <effect type="fade" start="100" end="60" time="250" />
                </animation>
                <include>InfoListRight</include>
                <control type="group">
                    <left>1065</left>
                    <top>180</top>
                    <control type="image">
                        <left>120</left>
                        <top>8</top>
                        <width>525</width>
                        <height>375</height>
                        <aspectratio>keep</aspectratio>
                        <fadetime>FadeTime</fadetime>
                        <texture background="true">$INFO[ListItem.Icon]</texture>
                        <bordertexture border="8">ThumbShadow.png</bordertexture>
                        <bordersize>8</bordersize>
                    </control>
                    <control type="group">
                        <top>405</top>
                        <control type="label">
                            <description>Born txt</description>
                            <left>0</left>
                            <top>0</top>
                            <width>210</width>
                            <height>38</height>
                            <label>$LOCALIZE[21893]:</label>
                            <align>right</align>
                            <aligny>center</aligny>
                            <textcolor>selected</textcolor>
                            <!-- <visible>!String.IsEmpty(ListItem.Property(Artist_Born))</visible> -->
                        </control>
                        <control type="label">
                            <description>Born Value</description>
                            <left>225</left>
                            <top>0</top>
                            <width>525</width>
                            <height>38</height>
                            <label>$INFO[ListItem.Property(Artist_Born)]</label>
                            <align>left</align>
                            <aligny>center</aligny>
                            <scroll>true</scroll>
                            <!-- <visible>!String.IsEmpty(ListItem.Property(Artist_Born))</visible> -->
                        </control>
                        <control type="label">
                            <description>Formed txt</description>
                            <left>0</left>
                            <top>38</top>
                            <width>210</width>
                            <height>38</height>
                            <label>$LOCALIZE[21894]:</label>
                            <align>right</align>
                            <aligny>center</aligny>
                            <textcolor>selected</textcolor>
                            <!-- <visible>!String.IsEmpty(ListItem.Property(Artist_Formed))</visible> -->
                        </control>
                        <control type="label">
                            <description>Formed Value</description>
                            <left>225</left>
                            <top>38</top>
                            <width>525</width>
                            <height>38</height>
                            <label>$INFO[ListItem.Property(Artist_Formed)]</label>
                            <align>left</align>
                            <aligny>center</aligny>
                            <font>font13</font>
                            <scroll>true</scroll>
                            <!-- <visible>!String.IsEmpty(ListItem.Property(Artist_Formed))</visible> -->
                        </control>
                        <control type="label">
                            <description>Genre txt</description>
                            <left>0</left>
                            <top>75</top>
                            <width>210</width>
                            <height>38</height>
                            <label>$LOCALIZE[515]:</label>
                            <align>right</align>
                            <aligny>center</aligny>
                            <textcolor>selected</textcolor>
                        </control>
                        <control type="label">
                            <description>Genre Value</description>
                            <left>225</left>
                            <top>75</top>
                            <width>525</width>
                            <height>38</height>
                            <label fallback="416">$INFO[ListItem.Property(Artist_Genre)]</label>
                            <align>left</align>
                            <aligny>center</aligny>
                            <scroll>true</scroll>
                        </control>
                        <control type="image">
                            <left>0</left>
                            <top>-15</top>
                            <width>765</width>
                            <height>3</height>
                            <colordiffuse>fanartdiffuse</colordiffuse>
                            <texture background="true" border="1">Separator1.png</texture>
                        </control>
                        <control type="image">
                            <left>0</left>
                            <top>123</top>
                            <width>765</width>
                            <height>3</height>
                            <colordiffuse>fanartdiffuse</colordiffuse>
                            <texture background="true" border="1">Separator1.png</texture>
                        </control>
                        <control type="textbox">
                            <description>Description Value for Artist</description>
                            <left>15</left>
                            <top>135</top>
                            <width>735</width>
                            <height>207</height>
                            <font>font16</font>
                            <textcolor>white</textcolor>
                            <label>$INFO[ListItem.Property(Artist_Description)]</label>
                        </control>
                    </control>
                </control>
            </control>
        </control>
        <control type="group">
            <include>openclose</include>
            <visible>Control.IsVisible(512)</visible>
            <include>CommonPageCount</include>
            <include condition="!Skin.HasSetting(HideJumpSMSBar)">AlphaBarV</include>
        </control>
    </include>
</includes>
