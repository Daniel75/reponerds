<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1401">
    <defaultcontrol always="true">2</defaultcontrol>
    <!-- <zorder>1</zorder> -->
    <visible>Player.HasVideo</visible>
    <controls>
        <control type="group">
            <left>0</left>
            <top>-8</top>
            <include>openclose</include>
            <include>opencloseslidedown</include>
            <visible>!Window.IsActive(visualisation)+!Window.IsActive(Home)+!Window.IsVisible(playercontrols)+!VideoPlayer.IsFullscreen+!Window.IsVisible(screencalibration)+![Window.IsVisible(videos)+Skin.HasSetting(CoverWallFull)]+!Control.HasFocus(666)</visible>
            <control type="group" id="501">
                <left>0</left>
                <top>-9</top>
                <control type="image">
                    <left>-15</left>
                    <top>4</top>
                    <width>1950</width>
                    <height>60</height>
                    <texture background="true">BlackBack.png</texture>
                </control>
                <control type="image">
                    <left>-15</left>
                    <top>64</top>
                    <width>1950</width>
                    <height>15</height>
                    <colordiffuse>menudiffuse</colordiffuse>
                    <texture background="true" flipy="true">Shadow.png</texture>
                </control>
                <control type="image">
                    <left>0</left>
                    <top>64</top>
                    <width>1920</width>
                    <height>1</height>
                    <texture background="true">Separator2.png</texture>
                </control>
                <control type="grouplist">
                    <left>15</left>
                    <top>16</top>
                    <width>1500</width>
                    <height>45</height>
                    <itemgap>3</itemgap>
                    <orientation>horizontal</orientation>
                    <include>RecordInfo</include>
                    <control type="image">
                        <!-- fake -->
                        <description>Separator</description>
                        <width>4</width>
                        <height>45</height>
                        <texture>-</texture>
                    </control>
                    <include>NavInfoPvrContent</include>
                    <control type="label">
                        <description>Navigation label</description>
                        <width>auto</width>
                        <label>$INFO[System.CurrentWindow]$INFO[Container.FolderName, - ,]</label>
                        <font>impacted24</font>
                        <textcolor>white</textcolor>
                        <visible>Skin.HasSetting(NavInfo)</visible>
                        <visible>!Window.IsActive(pvr)</visible>
                        <visible>!Window.IsActive(SkinSettings)</visible>
                    </control>
                    <control type="label">
                        <description>Navigation label</description>
                        <width>auto</width>
                        <label>20077</label>
                        <font>impacted24</font>
                        <textcolor>white</textcolor>
                        <visible>Window.IsActive(SkinSettings)</visible>
                    </control>
                    <control type="image">
                        <!-- fake -->
                        <description>Separator</description>
                        <width>8</width>
                        <height>45</height>
                        <texture>-</texture>
                    </control>
                    <control type="image">
                        <description>Separator</description>
                        <width>1</width>
                        <height>45</height>
                        <texture>Separator3.png</texture>
                    </control>
                    <control type="image">
                        <!-- fake -->
                        <description>Separator</description>
                        <width>0</width>
                        <height>45</height>
                        <texture>-</texture>
                    </control>
                    <control type="image">
                        <description>Overlay Watching</description>
                        <width>45</width>
                        <height>48</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$VAR[OSDTexture]</texture>
                    </control>
                    <control type="label">
                        <description>Now Playing Info</description>
                        <width>auto</width>
                        <height>51</height>
                        <textcolor>white</textcolor>
                        <shadowcolor>-</shadowcolor>
                        <label>$VAR[HomeOSDTitle]</label>
                        <scroll>true</scroll>
                    </control>
                    <control type="label">
                        <description>Now Playing Info</description>
                        <width>auto</width>
                        <height>51</height>
                        <textcolor>white</textcolor>
                        <shadowcolor>-</shadowcolor>
                        <label>$INFO[VideoPlayer.Time, [COLOR=description]â€¢[/COLOR]  , ]$INFO[VideoPlayer.Duration,[COLOR=grey]/ [/COLOR], ]</label>
                        <visible>!String.IsEmpty(Player.TimeRemaining)</visible>
                    </control>
                </control>
            </control>
            <control type="label">
                <animation effect="slide" end="-30" condition="Skin.HasSetting(BatteryLevel)">Conditional</animation>
                <left>478r</left>
                <top>6</top>
                <width>465</width>
                <align>right</align>
                <aligny>center</aligny>
                <font>impacted24</font>
                <label>$INFO[System.Time]</label>
            </control>
            <control type="group">
                <left>1872</left>
                <top>3</top>
                <width>36</width>
                <height>21</height>
                <include>BatteryLevel</include>
            </control>
        </control>
    </controls>
</window>
