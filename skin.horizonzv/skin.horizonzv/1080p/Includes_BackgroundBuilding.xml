    </include>
    <include name="DialogInfoBackgroundParam">
        <control type="group">
            <include>openclosevisible</include>
            <control type="image">
                <left>$PARAM[DialogBackgroundLeft]</left>
                <top>$PARAM[DialogBackgroundTop]</top>
                <width>$PARAM[DialogBackgroundWidth]</width>
                <height>$PARAM[DialogBackgroundHeight]</height>
                <colordiffuse>grey3</colordiffuse>
                <texture background="true" border="5">$VAR[SelectBack2]</texture>
                <!-- <texture background="true" border="5">Underlay2.png</texture> -->
            </control>
            <control type="label" id="$PARAM[DialogHeaderId]">
                <description>header label</description>
                <left>$PARAM[DialogBackgroundLeft]</left>
                <top>$PARAM[DialogHeaderTop]</top>
                <width>$PARAM[DialogBackgroundWidth]</width>
                <height>45</height>
                <font>impacted26</font>
                <label>$PARAM[DialogHeaderLabel]</label>
                <align>center</align>
                <aligny>center</aligny>
            </control>
            <control type="image">
                <left>$PARAM[DialogBackgroundLeft]</left>
                <top>$PARAM[DialogBackgroundTop]</top>
                <width>$PARAM[DialogBackgroundWidth]</width>
                <height>1</height>
                <texture>Separator2.png</texture>
            </control>
            <control type="image">
                <description>Dialog Header Separator image</description>
                <left>$PARAM[DialogBackgroundLeft]</left>
                <top>$PARAM[DialogSeparatorTop]</top>
                <width>$PARAM[DialogBackgroundWidth]</width>
                <height>1</height>
                <texture>Separator2.png</texture>
            </control>
            <control type="image">
                <left>$PARAM[DialogBackgroundLeft]</left>
                <top>$PARAM[DialogSeparatorBottom]</top>
                <width>$PARAM[DialogBackgroundWidth]</width>
                <height>1</height>
                <texture>Separator2.png</texture>
            </control>
            <control type="image">
                <description>mirror bottom image</description>
                <left>$PARAM[DialogBackgroundLeft]</left>
                <top>$PARAM[DialogShadowTop]</top>
                <width>$PARAM[DialogBackgroundWidth]</width>
                <height>82</height>
                <colordiffuse>fanartdiffuse</colordiffuse>
                <texture background="true" flipy="true" border="4">Shadow.png</texture>
            </control>
        </control>
    </include>
</includes>
