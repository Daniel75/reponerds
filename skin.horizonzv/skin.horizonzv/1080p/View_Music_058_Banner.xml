<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="BannerView_Music">
        <control type="group">
            <visible>Control.IsVisible(58)+!Control.HasFocus(666)</visible>
            <include>WhiteGrunge</include>
            <include>VideoGrid</include>
        </control>
        <control type="group">
            <include>openclosebox</include>
            <visible>Control.IsVisible(58)+!Control.HasFocus(666)</visible>
            <control type="fixedlist" id="58">
                <left>390</left>
                <top>105</top>
                <width>1125</width>
                <height>870</height>
                <onleft>9000</onleft>
                <onright condition="Skin.HasSetting(HideJumpSMSBar)+Control.IsVisible(60)">60</onright>
                <onright condition="Skin.HasSetting(HideJumpSMSBar)+!Control.IsVisible(60)">SetFocus(732)</onright>
                <onright condition="!Skin.HasSetting(HideJumpSMSBar)">15000</onright>
                <onup>58</onup>
                <ondown>58</ondown>
                <viewtype label="Banner">fixedlist</viewtype>
                <pagecontrol>60</pagecontrol>
                <focusposition>2</focusposition>
                <preloaditems>2</preloaditems>
                <movement>2</movement>
                <wraparound>true</wraparound>
                <scrolltime tween="Quadratic" easing="out">300</scrolltime>
                <visible>Container.Content(songs)|Container.Content(artists)|Container.Content(albums)</visible>
                <itemlayout width="840" height="90">
                    <control type="group">
                        <top>24</top>
                        <include>focuseffect</include>
                        <control type="label">
                            <left>60</left>
                            <width>825</width>
                            <label>$INFO[ListItem.Label]</label>
                            <shadowcolor>black</shadowcolor>
                        </control>
                        <control type="label">
                            <left>60</left>
                            <top>34</top>
                            <width>825</width>
                            <label>$VAR[InfoWallBottomLabel1Var]</label>
                            <textcolor>focused</textcolor>
                            <shadowcolor>black</shadowcolor>
                        </control>
                    </control>
                </itemlayout>
                <focusedlayout width="840" height="390">
                    <control type="group">
                        <include>focuseffect</include>
                        <top>30</top>
                        <control type="image">
                            <left>0</left>
                            <top>-30</top>
                            <width>1118</width>
                            <height>423</height>
                            <texture background="true" border="30">InlayDark.png</texture>
                        </control>
                        <control type="image">
                            <left>32</left>
                            <top>81</top>
                            <width>1054</width>
                            <height>195</height>
                            <aspectratio>keep</aspectratio>
                            <texture background="true">$INFO[ListItem.Icon]</texture>
                            <visible>ListItem.IsParentFolder</visible>
                        </control>
                        <control type="image" id="70">
                            <left>32</left>
                            <top>3</top>
                            <width>1054</width>
                            <height>360</height>
                            <aspectratio>center</aspectratio>
                            <colordiffuse>33FFFFFF</colordiffuse>
                            <texture border="1" background="true" diffuse="masks/mask.png">$INFO[ListItem.Art(fanart)]</texture>
                            <visible>!ListItem.IsParentFolder</visible>
                            <visible>Skin.HasSetting(fanart)</visible>
                        </control>
                        <control type="group">
                            <left>15</left>
                            <top>-8</top>
                            <visible>!ListItem.IsParentFolder</visible>
                            <control type="group" id="5">
                                <left>3</left>
                                <top>24</top>
                                <control type="group">
                                    <left>30</left>
                                    <top>98</top>
                                    <visible>!Skin.HasSetting(hideMusicCases)</visible>
                                    <control type="image">
                                        <description>case by resolution</description>
                                        <left>22</left>
                                        <top>0</top>
                                        <width>231</width>
                                        <height>231</height>
                                        <fadetime>FadeTime</fadetime>
                                        <texture background="true" diffuse="masks/CdCoverMask_full.png" fallback="FallbackCover.png">$INFO[ListItem.Icon]</texture>
                                        <aspectratio align="right" aligny="center">stretch</aspectratio>
                                    </control>
                                    <control type="image">
                                        <description>case by resolution</description>
                                        <left>0</left>
                                        <top>0</top>
                                        <width>255</width>
                                        <height>231</height>
                                        <fadetime>FadeTime</fadetime>
                                        <texture background="true">masks/ClearArtCD.png</texture>
                                        <aspectratio align="left" aligny="top">stretch</aspectratio>
                                    </control>
                                </control>
                                <control type="group">
                                    <visible>Skin.HasSetting(hideMusicCases)</visible>
                                    <control type="image" id="5">
                                        <left>36</left>
                                        <top>100</top>
                                        <width>225</width>
                                        <height>225</height>
                                        <texture background="true" diffuse="masks/mask.png" fallback="FallbackCover.png">$INFO[ListItem.Icon]</texture>
                                        <aspectratio align="right" aligny="center">stretch</aspectratio>
                                    </control>
                                </control>
                            </control>
                            <control type="group">
                                <top>24</top>
                                <control type="label">
                                    <left>38</left>
                                    <top>0</top>
                                    <width>1012</width>
                                    <textcolor>white</textcolor>
                                    <align>left</align>
                                    <label>$INFO[ListItem.Label]</label>
                                </control>
                                <control type="label">
                                    <left>38</left>
                                    <top>32</top>
                                    <width>1012</width>
                                    <align>left</align>
                                    <font>font16</font>
                                    <textcolor>focused</textcolor>
                                    <label>$VAR[InfoWallBottomLabel1Var]</label>
                                </control>
                            </control>
                            <control type="image">
                                <description>Rated</description>
                                <left>38</left>
                                <top>102</top>
                                <width>1012</width>
                                <height>3</height>
                                <colordiffuse>fanartdiffuse</colordiffuse>
                                <texture border="1" background="true">Separator1.png</texture>
                            </control>
                            <control type="textbox">
                                <left>300</left>
                                <top>120</top>
                                <width>750</width>
                                <height>225</height>
                                <include condition="!Skin.HasSetting(BigPlotText)">PlotText16Var</include>
                                <include condition="Skin.HasSetting(BigPlotText)">PlotTextBigVar</include>
                                <label fallback="195">$VAR[InfoWallTextboxVar]</label>
                            </control>
                        </control>
                    </control>
                </focusedlayout>
            </control>
            <control type="group">
                <left>315</left>
                <top>88</top>
                <control type="image">
                    <left>0</left>
                    <top>0</top>
                    <width>1275</width>
                    <height>18</height>
                    <colordiffuse>dustdiffuse</colordiffuse>
                    <texture background="true" diffuse="Separator2.png">Shadow.png</texture>
                </control>
                <control type="image">
                    <left>0</left>
                    <top>15</top>
                    <width>1275</width>
                    <height>1</height>
                    <colordiffuse>dustdiffuse</colordiffuse>
                    <texture background="true">Separator2.png</texture>
                </control>
                <control type="group">
                    <left>0</left>
                    <top>878</top>
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>1275</width>
                        <height>21</height>
                        <colordiffuse>dustdiffuse</colordiffuse>
                        <texture flipx="true" background="true" diffuse="Separator2.png">Shadow.png</texture>
                    </control>
                    <control type="image">
                        <left>0</left>
                        <top>9</top>
                        <width>1275</width>
                        <height>1</height>
                        <colordiffuse>dustdiffuse</colordiffuse>
                        <texture background="true">Separator2.png</texture>
                    </control>
                </control>
            </control>
        </control>
        <control type="group">
            <include>openclosevisible</include>
            <visible>Control.IsVisible(58)+!Control.HasFocus(666)</visible>
            <control type="scrollbar" id="60">
                <left>1500</left>
                <top>105</top>
                <height>870</height>
                <onleft>58</onleft>
                <onright>732</onright>
                <showonepage>false</showonepage>
                <orientation>vertical</orientation>
            </control>
            <control type="group">
                <left>1461</left>
                <top>186</top>
                <control type="group">
                    <visible>Skin.HasSetting(fanart)</visible>
                    <control type="image">
                        <include>openclosevisible</include>
                        <left>141</left>
                        <top>532</top>
                        <width>315</width>
                        <height>172</height>
                        <fadetime>FadeTime</fadetime>
                        <texture background="true">Black.png</texture>
                        <visible>![String.IsEmpty(Control.GetLabel(1099))+String.IsEmpty(Control.GetLabel(1100))]</visible>
                    </control>
                    <control type="multiimage" id="1099">
                        <include>openclosevisible</include>
                        <left>141</left>
                        <top>537</top>
                        <width>304</width>
                        <height>164</height>
                        <aspectratio>keep</aspectratio>
                        <imagepath background="true">$INFO[Skin.String(Music_Folder_Path)]$INFO[ListItem.Artist,,/extrafanart]</imagepath>
                        <timeperimage>5000</timeperimage>
                        <randomize>true</randomize>
                        <fadetime>FadeTime</fadetime>
                        <visible>Skin.HasSetting(music_art)</visible>
                    </control>
                    <control type="image" id="1100">
                        <include>openclosevisible</include>
                        <left>141</left>
                        <top>537</top>
                        <width>304</width>
                        <height>164</height>
                        <aspectratio align="left" aligny="bottom">scale</aspectratio>
                        <fadetime>FadeTime</fadetime>
                        <texture background="true">$INFO[ListItem.Art(fanart)]</texture>
                        <visible>!String.IsEmpty(ListItem.Art(fanart))+String.IsEmpty(Control.GetLabel(1099))</visible>
                    </control>
                </control>
                <control type="group">
                    <visible>Skin.HasSetting(music_art)</visible>
                    <control type="image">
                        <include>openclosevisible</include>
                        <left>135</left>
                        <top>0</top>
                        <width>315</width>
                        <height>170</height>
                        <fadetime>FadeTime</fadetime>
                        <texture background="true">Black.png</texture>
                        <visible>!String.IsEmpty(Control.GetLabel(1101))</visible>
                    </control>
                    <control type="image" id="1101">
                        <include>openclosevisible</include>
                        <left>141</left>
                        <top>6</top>
                        <width>303</width>
                        <height>162</height>
                        <fadetime>FadeTime</fadetime>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.Property(Album_Label),flagging/recordlabel/,.png]</texture>
                    </control>
                </control>
            </control>
            <control type="group">
                <top>292</top>
                <left>3</left>
                <visible>Skin.HasSetting(music_art)</visible>
                <control type="image">
                    <include>openclosevisible</include>
                    <left>0</left>
                    <top>68</top>
                    <width>318</width>
                    <height>172</height>
                    <fadetime>FadeTime</fadetime>
                    <texture background="true">Black.png</texture>
                    <visible>!String.IsEmpty(Control.GetLabel(1102))</visible>
                </control>
                <control type="image" id="1102">
                    <include>openclosevisible</include>
                    <left>12</left>
                    <top>76</top>
                    <width>302</width>
                    <height>160</height>
                    <aspectratio>keep</aspectratio>
                    <fadetime>FadeTime</fadetime>
                    <texture background="true" diffuse="masks/flagging_mask.png">$INFO[Skin.String(Music_Folder_Path)]$INFO[ListItem.Artist,,/logo.png]</texture>
                </control>
            </control>
            <control type="group">
                <include>opencloseslideup</include>
                <visible>!ListItem.IsParentFolder</visible>
                <left>0</left>
                <top>1005</top>
                <control type="image">
                    <left>-15</left>
                    <top>-15</top>
                    <width>1950</width>
                    <height>15</height>
                    <colordiffuse>fanartdiffuse</colordiffuse>
                    <texture background="true">Shadow.png</texture>
                </control>
                <control type="image">
                    <left>-15</left>
                    <top>0</top>
                    <width>1950</width>
                    <height>112</height>
                    <texture background="true">BlackBack.png</texture>
                </control>
                <control type="image">
                    <left>0</left>
                    <top>0</top>
                    <width>1920</width>
                    <height>1</height>
                    <texture background="true">Separator2.png</texture>
                </control>
            </control>
            <control type="group">
                <!-- Flagging -->
                <left>30</left>
                <top>15</top>
                <include>opencloseslideup</include>
                <visible>!ListItem.IsParentFolder</visible>
                <control type="group">
                    <left>0</left>
                    <top>998</top>
                    <control type="grouplist">
                        <left>0</left>
                        <top>6</top>
                        <width>1860</width>
                        <height>120</height>
                        <itemgap>60</itemgap>
                        <align>center</align>
                        <orientation>horizontal</orientation>
                        <include>FlagMusic</include>
                    </control>
                </control>
            </control>
            <control type="group">
                <left>0</left>
                <top>9</top>
                <include>CommonPageCount</include>
            </control>
            <include condition="!Skin.HasSetting(HideJumpSMSBar)">AlphaBarV</include>
        </control>
    </include>
</includes>
