        <width>580</width>
        <height>1120</height>
        <texture background="true">skin/button_klein_tranz90.png</texture>
		<bordertexture border="10">ThumbShadow.png</bordertexture>
		<bordersize>7</bordersize>
	    <colordiffuse>$VAR[ColorButtonDialogeBack]</colordiffuse>
      </control>
	  <control type="image">
		<left>-10</left>
		<top>1</top>
		<width>552</width>
		<height>89</height>
		<texture background="true">skin/button_klein_tranz70.png</texture>
		<colordiffuse>$VAR[BackColorButton]</colordiffuse>
	  </control>
      <control type="button">
        <description>Close Window button</description>
        <left>565</left>
        <top>20</top>
        <width>48</width>
        <height>48</height>
        <label>-</label>
        <font>-</font>
        <onclick>PreviousMenu</onclick>
		<texturefocus>default/close.png</texturefocus>
        <texturenofocus>default/close.png</texturenofocus>
        <onleft>2</onleft>
        <onright>2</onright>
        <onup>2</onup>
        <ondown>2</ondown>
        <visible>!Skin.HasSetting(tablet_modus)</visible>
      </control>
      <control type="label">
        <description>header label</description>
        <left>25</left>
        <top>20</top>
        <width>490</width>
        <height>45</height>
        <font>primal_20</font>
        <label>$LOCALIZE[19029] - $INFO[VideoPlayer.ChannelName] - ($INFO[Container(11).NumItems]) $LOCALIZE[49971] - $LOCALIZE[49970] ($INFO[Container(11).CurrentPage]/$INFO[Container(11).NumPages])</label>
        <align>left</align>
        <aligny>center</aligny>
		<scroll>true</scroll>
        <textcolor>$VAR[ColorFontSelected]</textcolor>
	    <shadowcolor>$VAR[ColorFontShadow]</shadowcolor>
      </control>
      <control type="list" id="11">
        <left>-10</left>
		<top>90</top>
		<width>553</width>
		<height>1000</height>
		<onleft>PreviousChannelGroup</onleft>
		<onright>NextChannelGroup</onright>
		<onup>11</onup>
		<ondown>11</ondown>
		<viewtype label="535">list</viewtype>
		<pagecontrol>60</pagecontrol>
		<scrolltime>200</scrolltime>
		<itemlayout height="75" width="610">
		  <control type="image">
			<left>-10</left>
			<top>0</top>
			<width>573</width>
