            <top>20</top>
            <width>45</width>
            <height>30</height>
            <texture>PVR-IsRecording.png</texture>
            <visible>ListItem.IsRecording</visible>
          </control>
		  <control type="image">
            <left>30</left>
            <top>5</top>
            <width>60</width>
            <height>60</height>
            <texture>$INFO[ListItem.Icon]</texture>
			<aspectratio>keep</aspectratio>
          </control>
        </focusedlayout>
