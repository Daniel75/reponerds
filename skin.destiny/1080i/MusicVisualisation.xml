<?xml version="1.0" encoding="UTF-8"?>
<window id="2006">
  <defaultcontrol always="true">999</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <onload condition="system.hasaddon(script.artistslideshow) + IsEmpty(Window(Home).Property(ArtistSlideShowLoaded))">RunScript(script.artistslideshow, daemon=True)</onload>
  <controls>
    <control type="visualisation" id="2">
      <left>0</left>
      <top>0</top>
      <width>1920</width>
      <height>1080</height>
	  <visible>Skin.HasSetting(musikvisualisation)</visible>
    </control>
    <control type="image">
      <description>Default background</description>
      <left>0</left>
      <top>0</top>
      <width>1920</width>
      <height>1080</height>
      <colordiffuse>88FFFFFF</colordiffuse>
      <visible>!Skin.HasSetting(CustomBackground) + !Skin.HasSetting(BlackBackground) + !Skin.HasSetting(ArtistslideshowHome) + !Skin.HasSetting(CDWallNoVisualisation)</visible>
      <texture fallback="special://skin/backgrounds/SKINDEFAULT.jpg">$INFO[MusicPlayer.Property(Fanart_Image)]</texture>
      <animation effect="zoom" start="110" end="135" center="auto" time="15000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg_global)">Conditional</animation>
      <animation effect="slide" end="-30,-30" delay="4000" time="8000" tween="sine" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg_global)">Conditional</animation>
      <animation effect="slide" end="60,0" delay="10000" time="6000" tween="sine" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg_global)">Conditional</animation>
    </control>
    <control type="multiimage">
      <left>0</left>
	  <top>0</top>
	  <width>1920</width>
	  <height>1080</height>
	  <aspectratio scalediffuse="false" align="center" aligny="top">scale</aspectratio>
	  <imagepath background="true">$INFO[Window(Visualisation).Property(ArtistSlideshow)]</imagepath>
	  <timeperimage>10000</timeperimage>
	  <fadetime>2000</fadetime>
	  <randomize>true</randomize>
      <!--<animation effect="fade" start="0" end="100" time="300">Visible</animation>
      <animation effect="fade" start="100" end="0" time="300">Hidden</animation>
      <animation effect="zoom" start="110" end="130" center="auto" time="6000" pulse="true" condition="true">Conditional</animation>
      <animation effect="slide" start="-30,-30" end="60,60" time="7000" pulse="true" condition="true">Conditional</animation>-->
      <visible>IsEmpty(Window.Property(ArtistSlideshowRefresh))</visible>
      <visible>Skin.HasSetting(Artistslideshow)</visible>
      <visible>!Skin.HasSetting(BlackBackground)</visible>
      <visible>!Skin.HasSetting(CustomBackground)</visible>
    </control>
    <control type="multiimage">
