				<left>0</left>
				<top>0</top>
				<width>240</width>
				<height>158</height>
				<texture border="15">RecentAddedBack.png</texture>
				<visible>Player.HasMedia+!Control.HasFocus(58051)</visible>
			</control>
			<control type="image">
				<description>background</description>
				<left>0</left>
				<top>0</top>
				<width>240</width>
				<height>310</height>
