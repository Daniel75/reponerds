<?xml version='1.0' encoding='UTF-8'?>
<includes>	
	<include name="Landscape">
		<control type="group">
			<visible>Control.IsVisible(723)</visible>
			<include>VisibleFadeEffect</include>
			<left>0</left>
			<top>0</top>
			<control type="image">
				<left>-100</left>
				<top>-80</top>
				<width>1520</width>
				<height>950</height>
				<texture>ContentPanel.png</texture>
				<visible>Control.IsVisible(723)</visible>
			</control>
			<control type="image">
				<left>-20</left>
				<top>430</top>
				<width>1350</width>
				<height>420</height>
				<aspectratio>stretch</aspectratio>
				<texture>ContentPanel.png</texture>
			</control>
			<control id="723" type="wraplist">
				<visible>Container.Content(Episodes)</visible>
				<left>10</left>
				<top>360</top>
				<width>1926</width>
				<height>460</height>
				<onleft>57</onleft>
				<onright>57</onright>
				<onup>2</onup>
				<ondown>60</ondown>
				<viewtype label="Landscape">bigwrap</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>150</scrolltime>
				<focusposition>3</focusposition>
				<orientation>horizontal</orientation>
				<preloaditems>4</preloaditems>
				<itemlayout height="176" width="330">
					<control type="image">
						<left>-500</left>
						<top>100</top>
						<width>300</width>
						<height>186</height>
						<aspectratio>stretch</aspectratio>
						<texture background="true" diffuse="episodematte.png">$INFO[ListItem.Icon]</texture>
					</control>
					<control type="image">
						<left>-500</left>
						<top>257</top>
						<width>300</width>
						<height>30</height>
						<texture>tv-episodes/epi-black.png</texture>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="label">
						<left>-552</left>
						<top>255</top>
						<width>235</width>
						<label>$LOCALIZE[20373] $INFO[ListItem.Season]</label>
						<align>center</align>
						<font>cirrus_22</font>
						<textcolor>ffffffff</textcolor>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>	
					<control type="label">
						<left>-387</left>
						<top>255</top>
						<width>235</width>
						<label>$LOCALIZE[20452] $INFO[ListItem.Episode]</label>
						<align>center</align>
						<font>cirrus_22</font>
						<textcolor>ffffffff</textcolor>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="image">
						<left>-515</left>
						<top>85</top>
						<width>330</width>
						<height>215</height>
						<texture>episodeframe.png</texture>
						<colordiffuse>grey3</colordiffuse>
					</control>				
					<control type="image">
						<visible>Container.Content(episodes)</visible>
						<left>-365</left>
						<top>252</top>
						<width>27</width>
						<height>36</height>
						<texture>$INFO[ListItem.Overlay]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="176" width="330">
					<control type="image">
						<left>-500</left>
						<top>100</top>
						<width>300</width>
						<height>186</height>
						<aspectratio>stretch</aspectratio>
						<texture background="true" diffuse="episodematte.png">$INFO[ListItem.Icon]</texture>
					</control>
					<control type="image">
						<left>-500</left>
						<top>257</top>
						<width>300</width>
						<height>30</height>
						<texture>tv-episodes/epi-black.png</texture>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="label">
						<left>-552</left>
						<top>255</top>
						<width>235</width>
						<label>$LOCALIZE[20373] $INFO[ListItem.Season]</label>
						<align>center</align>
						<font>cirrus_22</font>
						<textcolor>ffffffff</textcolor>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>				
					<control type="label">
						<left>-387</left>
						<top>255</top>
						<width>235</width>
						<label>$LOCALIZE[20452] $INFO[ListItem.Episode]</label>
						<align>center</align>
						<font>cirrus_22</font>
						<textcolor>ffffffff</textcolor>
						<visible>!IsEmpty(ListItem.Title)</visible>
					</control>
					<control type="image">
						<left>-515</left>
						<top>85</top>
						<width>330</width>
						<height>215</height>
						<texture>episodeframe.png</texture>
						<colordiffuse>blue</colordiffuse>
					</control>				
					<control type="image">
						<visible>Container.Content(episodes)</visible>
						<left>-365</left>
						<top>252</top>
						<width>27</width>
						<height>36</height>
						<texture>$INFO[ListItem.Overlay]</texture>
					</control>
				</focusedlayout>
			</control>
			<control type="group">
				<visible>Control.IsVisible(723) + Container.Content(episodes)</visible>
				<visible>!ListItem.IsFolder</visible>
				<animation delay="1500" effect="fade" time="3000">Visible</animation>
				<animation delay="0" effect="fade" time="300">Hidden</animation>
				<control type="label">
					<description>Title text</description>
					<left>253</left>
					<top>660</top>
					<width>800</width>
					<height>33</height>
					<visible allowhiddenfocus="true">Control.IsVisible(723) + Control.HasFocus(723)</visible>
					<font>font35_title</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
					<scroll>true</scroll>
					<align>center</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Title]</label>
					<animation delay="0" effect="fade" time="300">focus</animation>
					<animation delay="0" effect="fade" time="300">unfocus</animation>
				</control>
				<control type="group">
					<left>40</left>
					<control type="label">
						<left>25</left>
						<top>165</top>
						<width>600</width>
						<height>35</height>
						<font>font13</font>
						<textcolor>white</textcolor>
						<scroll>true</scroll>
						<align>left</align>
						<aligny>center</aligny>
						<label>[COLOR blue]$LOCALIZE[20417]:[/COLOR] $INFO[ListItem.Writer]</label>
					</control>
					<control type="textbox">
						<description>Plot</description>
						<left>25</left>
						<top>300</top>
						<width>600</width>
						<height>120</height>
						<font>font13</font>
						<align>justify</align>
						<textcolor>white</textcolor>
						<label>$INFO[ListItem.Plot,[COLOR blue]$LOCALIZE[207]: [/COLOR]]</label>
						<autoscroll delay="3000" repeat="5000" time="2000">Skin.HasSetting(AutoScroll) + Control.IsVisible(723)</autoscroll>
					</control>	
					<control type="label">
						<left>25</left>
						<top>235</top>
						<width>600</width>
						<height>35</height>
						<font>font13</font>
						<textcolor>white</textcolor>
						<scroll>true</scroll>
						<align>left</align>
						<aligny>center</aligny>
						<label>[COLOR blue]$LOCALIZE[20416]:[/COLOR] $INFO[ListItem.Date]</label>
					</control>
				</control>
				<control type="image">
					<left>730</left>
					<top>90</top>
					<width>512</width>
					<height>288</height>
					<texture>$INFO[ListItem.Art(fanart)]</texture>
					<animation delay="1500" effect="fade" time="3000">WindowOpen</animation>
				</control>
				<control type="image">
					<left>405</left>
					<top>-62</top>
					<width>1107</width>
					<height>635</height>
					<texture>tv-episodes/episode-glow.png</texture>
					<animation delay="1500" effect="fade" time="3000">WindowOpen</animation>
				</control>
				<control type="image">
					<left>150</left>
					<top>45</top>
					<width>400</width>
					<height>110</height>
					<texture background="false">$INFO[ListItem.Art(tvshow.clearlogo)]</texture>
					<aspectratio>keep</aspectratio>
					<animation delay="1500" effect="fade" time="3000">WindowOpen</animation>
				</control>
				<control type="group">
					<control type="group">
						<left>-860</left>
						<top>390</top>
						<control type="grouplist">
							<description>MPAA Flagging Conditions Images</description>
							<left>1300</left>
							<top>0</top>
							<width>800</width>
							<align>right</align>
							<itemgap>1</itemgap>
							<orientation>horizontal</orientation>
							<include>VideoCodecFlaggingConditions</include>
							<include>AudioCodecFlaggingConditions</include>
							<include>AudioChannelsFlaggingConditions</include>
							<include>AspectCodecFlaggingConditions</include>
							<include>VideoStereoscopicsFlaggingConditions</include>
							<include>VideoTypeHackFlaggingConditions</include>
							<include>VideoMPAAFlaggingConditions</include>
							<include>VideoFSKFlaggingConditions</include>
							<include>VideoBBFCFlaggingConditions</include>
						</control>
					</control>
					<control type="image" id="9898">
						<animation delay="500" effect="slide" end="0,100" start="0,0" time="500">WindowClose</animation>
						<animation delay="500" effect="slide" end="0,0" start="0,100" time="500">WindowOpen</animation>
						<width>153</width>
						<height>98</height>
						<left>0</left>
						<top>638</top>
						<aspectratio>keep</aspectratio>
						<align>center</align>
						<aligny>center</aligny>
						<texture>$VAR[MovieStudioLogoVar]$INFO[ListItem.Studio,,.png]</texture>
						<visible>!IsEmpty(ListItem.Studio)</visible>
					</control>
				</control>
			</control>
		</control>
	</include>
</includes>